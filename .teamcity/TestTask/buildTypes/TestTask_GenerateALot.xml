<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="3f71790d-d01d-4f41-8da1-1fc3d33b0752" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2021.1/project-config.xsd">
  <name>generate-a-lot</name>
  <description />
  <settings>
    <options>
      <option name="artifactRules" value="*.txt" />
    </options>
    <parameters />
    <build-runners>
      <runner id="RUNNER_4" name="" type="simpleRunner">
        <parameters>
          <param name="script.content"><![CDATA[for i in {1..100};
do
TIMESTAMP=`gdate “+%%Y-%%m-%%d %%H:%%M:%%S:%%N”`
echo “Writing $i $TIMESTAMP into file$i.txt”
echo “$i $TIMESTAMP”>> file$i.txt
done]]></param>
          <param name="teamcity.step.mode" value="default" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="TestTask_HttpsGithubComDaimLiatTestTaskGitRefsHeadsMain" />
    </vcs-settings>
    <requirements />
    <build-triggers />
    <cleanup />
  </settings>
</build-type>

